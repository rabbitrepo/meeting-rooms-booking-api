{
  "name": "dohome-booking",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "dev": "pnpm start:dev",
    "start": "concurrently \"yarn start:api-gateway\" \"yarn start:users\" \"yarn start:bookings\" \"yarn start:resources\"",
    "start:dev": "concurrently \"yarn start:dev:api-gateway\" \"yarn start:dev:users\" \"yarn start:dev:bookings\" \"yarn start:dev:resources\"",
    "start:debug": "concurrently \"yarn start:debug:api-gateway\" \"yarn start:debug:users\" \"yarn start:debug:bookings\" \"yarn start:debug:resources\"",
    "start:prod": "concurrently \"yarn start:prod:api-gateway\" \"yarn start:prod:users\" \"yarn start:prod:bookings\" \"yarn start:prod:resources\"",
    "start:api-gateway": "nest start api-gateway",
    "start:dev:api-gateway": "nest start api-gateway --watch",
    "start:debug:api-gateway": "nest start api-gateway --debug --watch",
    "start:prod:api-gateway": "node dist/apps/api-gateway/main",
    "start:users": "nest start users",
    "start:dev:users": "nest start users --watch",
    "start:debug:users": "nest start users --debug --watch",
    "start:prod:users": "node dist/apps/users/main",
    "start:bookings": "nest start bookings",
    "start:dev:bookings": "nest start bookings --watch",
    "start:debug:bookings": "nest start bookings --debug --watch",
    "start:prod:bookings": "node dist/apps/bookings/main",
    "start:resources": "nest start resources",
    "start:dev:resources": "nest start resources --watch",
    "start:debug:resources": "nest start resources --debug --watch",
    "start:prod:resources": "node dist/apps/resources/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/dohome-booking/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.4.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^7.0.0",
    "express": "^4.21.0",
    "mongoose": "^8.6.2",
    "node-appwrite": "^14.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.1.0",
    "sanitize-html": "^2.13.0",
    "ts-loader": "^9.5.1"
  }
}
